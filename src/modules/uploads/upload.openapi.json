    "paths": {
        "/Upload/do_spaces_file_upload": {
            "post": {
                "tags": [
                    "Upload"
                ],
                "summary": "File Upload Api",
                "requestBody": {
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "file": {
                                        "type": "string",
                                        "format": "binary"
                                    }
                                },
                                "required": [
                                    "file"
                                ]
                            }
                        }
                    }
                },
                "responses": {
                    "default": {
                        "description": "successful operation"
                    }
                }
            }
        },
        "/Upload/do_spaces_file_upload_multiple": {
            "post": {
